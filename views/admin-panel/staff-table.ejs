<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>view-contact-file</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

</head>
<body class="category-custom">

    <!-- ===================================== -->
    <%- include('./header.ejs')%>
    <!-- ===================================== -->

    <!-- ===================================== -->
    <%- include('./left.ejs')%>
    <!-- ===================================== -->

    <div class="col-md-9">
        <div class="container">
    <% if(stafftable.length>0){ %>
    
    <table id="brandTable">
        <thead>
            <tr>
                <th>Serial No.</th>
                <th>Id</th>
                <th>Name</th>
                <th>Phone no.</th>
                <th>Address</th>
                <th>Region</th>
                <th>Actions</th>


               
            </tr>
        </thead>
        <tbody>
            <%for(var i=0; i<stafftable.length;i++){%>
            <tr>
                <td><%= stafftable[i].serial %></td>
                <td><%= stafftable[i].identity %></td>
                <td><%= stafftable[i].name %></td>
                <td><%= stafftable[i].phone %></td>
                <td><%= stafftable[i].address %></td>
                <td><%= stafftable[i].region %></td>
                <td>
                    <a href="/delete5/<%= stafftable[i]._id %>"><button class="button1">Delete</button></a>&nbsp;
                    <a href="/edit5/<%= stafftable[i]._id %>" ><button class="button2">Edit</button></a>
    
                </td>


             
            </tr>
            <% } %>
        </tbody>
    </table>
    
    <% } else { %>
        no record found
    
        <%} %>
        </div>
    </div>
    </div>
    

    <!-- ===================================== -->
    <%- include('./footer.ejs')%>
    <!-- ===================================== -->
    
</body>
</html>