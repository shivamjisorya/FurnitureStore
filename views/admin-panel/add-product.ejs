
    



    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>add-product-file</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
        
    
    </head>
    <body>
    
        <!-- ===================================== -->
        <%- include('./header.ejs')%>
        <!-- ===================================== -->
    
        <!-- ===================================== -->
        <%- include('./left.ejs')%>
        <!-- ===================================== -->
        <div class="col-md-9 form-container-body">
    
            <div class="form-container">
                <form action="/addproduct" method="post" enctype="multipart/form-data">
                    <h1>Add Product Details</h1>

                    <div class="form-group">
                        <label for="productName">Product Image:</label>
                        <input type="file" id="productImage" name="filename">
                    </div>

                    <div class="form-group">
                        <label for="productName">Product Name:</label>
                        <input type="text" id="productName" name="productname">
                    </div>
        
                    <div class="form-group">
                        <label for="productType">Product Type:</label>
                        <input type="text" id="productType" name="producttype">
                    </div>
        
                    <div class="form-group">
                        <label for="productPrice">Product Price:</label>
                        <input type="number" id="productPrice" name="productprice">
                    </div>
        
                    <div class="form-group">
                        <label for="productQuantity">Product Description:</label>
                        <input type="text" id="productQuantity" name="description">
                    </div>

                    <div class="form-group">
                        <label for="productQuantity">Product Highlights:</label>
                        <input type="text" id="productQuantity" name="highlights">
                    </div>
        
                    <div class="form-group">
                        <button type="submit">Submit</button>
                    </div>
                </form>
            </div>
            </div>
            </div>
        
    
    
        <!-- ===================================== -->
        <%- include('./footer.ejs')%>
        <!-- ===================================== -->
        
    </body>
    </html>