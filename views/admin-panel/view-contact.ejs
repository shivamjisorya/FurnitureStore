<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>view-contact-file</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

</head>
<body class="category-custom">

    <!-- ===================================== -->
    <%- include('./header.ejs')%>
    <!-- ===================================== -->

    <!-- ===================================== -->
    <%- include('./left.ejs')%>
    <!-- ===================================== -->

    <div class="col-md-9">
        <div class="container">
    <% if(contactdata.length>0){ %>
    
    <table id="brandTable">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Subject</th>
                <th>Message</th>
                <th>Actions</th>
               
            </tr>
        </thead>
        <tbody>
            <%for(var i=0; i<contactdata.length;i++){%>
            <tr>
                <td><%= contactdata[i].name %></td>
                <td><%= contactdata[i].email %></td>
                <td><%= contactdata[i].subject %></td>
                <td><%= contactdata[i].message %></td>
                <td>
                    <a href="/delete3/<%= contactdata[i]._id %>"><button class="button1">Delete</button></a>&nbsp;
                    <a href="/edit3/<%= contactdata[i]._id %>" ><button class="button2">Edit</button></a>
    
                </td>
             
            </tr>
            <% } %>
        </tbody>
    </table>
    
    <% } else { %>
        no record found
    
        <%} %>
        </div>
    </div>
    </div>
    

    <!-- ===================================== -->
    <%- include('./footer.ejs')%>
    <!-- ===================================== -->
    
</body>
</html>