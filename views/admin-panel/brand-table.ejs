
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>brand-table-file</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    

</head>
<body class="category-body">

    <!-- ===================================== -->
    <%- include('./header.ejs')%>
    <!-- ===================================== -->

    <!-- ===================================== -->
    <%- include('./left.ejs')%>
    <!-- ===================================== -->
    
    <div class="col-md-9">
        <div class="container">
    <% if(brandstable.length>0){ %>
    
    <table id="brandTable">
        <thead>
            <tr>
            <th>S.No</th>
            <th>ID</th>
            <th>Name</th>
            <th>Contract Years</th>
            <th>
                Actions
            </th>
        </tr>
        </thead>
        <tbody>
            <%for(var i=0; i<brandstable.length;i++){%>
            <tr>
                <td><%= brandstable[i].serial %></td>
                <td><%= brandstable[i].identity %></td>
                <td><%= brandstable[i].name %></td>
                <td><%= brandstable[i].years %></td>
                <td>
                    <a href="/delete6/<%= brandstable[i]._id %>"><button class="button1">Delete</button></a>&nbsp;
                    <a href="/edit6/<%= brandstable[i]._id %>" ><button class="button2">Edit</button></a>
    
                </td>

            
            </tr>
            <% } %>
        </tbody>
    </table>
    
    <% } else { %>
        no record found
    
        <%} %>
        </div>
    </div>
    </div>
    

<!-- ===================================== -->
<%- include('./footer.ejs')%>
<!-- ===================================== -->

</body>
</html>