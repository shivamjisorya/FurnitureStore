    <script>
        function showCategory(category) {
          // Hide all category cards
          document.querySelectorAll('.row').forEach(row => row.style.display = 'none');
    
          // Show the selected category cards
          document.getElementById(`pet-card-container-${category}`).style.display = 'flex';
    
          // Update active state in the navbar
          document.querySelectorAll('.pet-nav-item').forEach(item => item.classList.remove('pet-active'));
          document.querySelector(`.pet-nav-item[data-category="${category}"]`).classList.add('pet-active');
        }
      </script>
