  <script>
    function openOffCanvas() {
        // Display the off-canvas
        document.getElementById('offCanvas').style.display = 'block';
    }

    function closeOffCanvas() {
        // Hide the off-canvas
        document.getElementById('offCanvas').style.display = 'none';
